{
    "application": {
        "name": "Jitsi Meet Qt",
        "version": "2.0.0",
        "language": "en",
        "theme": "light",
        "autoStart": false,
        "minimizeToTray": true,
        "closeToTray": false,
        "checkUpdates": true
    },
    "conference": {
        "defaultServer": "meet.jit.si",
        "displayName": "",
        "email": "",
        "autoJoinAudio": true,
        "autoJoinVideo": true,
        "startWithAudioMuted": false,
        "startWithVideoMuted": false,
        "enableChat": true,
        "enableScreenShare": true,
        "enableRecording": false
    },
    "audio": {
        "inputDevice": "default",
        "outputDevice": "default",
        "microphoneGain": 1.0,
        "speakerVolume": 1.0,
        "enableEchoCancellation": true,
        "enableNoiseSuppression": true,
        "enableAutoGainControl": true,
        "sampleRate": 48000,
        "channels": 2
    },
    "video": {
        "inputDevice": "default",
        "resolution": {
            "width": 1280,
            "height": 720
        },
        "frameRate": 30,
        "quality": "medium",
        "enableHardwareAcceleration": true,
        "mirrorLocalVideo": true
    },
    "network": {
        "useStun": true,
        "stunServers": [
            "stun:stun.l.google.com:19302",
            "stun:stun1.l.google.com:19302"
        ],
        "useTurn": false,
        "turnServers": [],
        "enableP2P": true,
        "maxBitrate": 2500000,
        "minBitrate": 150000
    },
    "ui": {
        "showToolbar": true,
        "showParticipantsList": true,
        "showChatPanel": true,
        "enableFullscreen": true,
        "windowGeometry": {
            "x": 100,
            "y": 100,
            "width": 1200,
            "height": 800
        }
    },
    "privacy": {
        "enableAnalytics": false,
        "enableCrashReports": false,
        "enableUsageStats": false
    },
    "advanced": {
        "enableDebugMode": false,
        "logLevel": "info",
        "enableMetrics": false,
        "customCss": "",
        "customJs": ""
    }
}